{
  "expo": {
    "owner": "heylinda",
    "name": "Hey Linda",
    "slug": "hey-linda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",

    "plugins": [
      [
        "expo-splash-screen",
        {
          "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#fff",
            "dark": {
              "image": "./assets/images/splash.png",
              "resizeMode": "contain",
              "backgroundColor": "#121212"
            }
          }
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      },
      "bundleIdentifier": "com.meditation.heylinda"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.meditation.heylinda",
      "versionCode": 8
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
